<?xml version="1.0" encoding="UTF-8"?>
<!-- This file defines the default (factory) configuration use by ListProducts service. The configuration defines the products that are 
    know to the system. -->
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.appliedindustriallogic.com/schemas/Configuration.xsd">
    <name>ProductCatalog configuration</name>
    <timeout>60000</timeout>
    <who>factory</who>
    <version>$Revision: 1.2 $</version>
    <manager>com.ail.core.product.listproducts.ListProductsService</manager>

    <group name="products">
        <parameter name="AIL.Base">Root of the product tree. All products extend this product.</parameter>
        <parameter name="AIL.Demo.MotorPlus">Demonstration of a multi-section, multi-asset product.</parameter>
        <parameter name="AIL.Demo.EquinePlus">Demonstration of a multi-section, multi-asset product.</parameter>
        <parameter name="AIL.Demo.IrishBrokerPI">Professional Indemnity product developed in conjunction with a London City broker.</parameter>
        <parameter name="AIL.Demo.LifePlus">Very simplistic term life product.</parameter>
        <parameter name="AIL.Demo.FreightPlus">International freight shipping product.</parameter>
        <parameter name="AIL.Demo.TradePL.GenericQB">Abstract Trade Public Liability product</parameter>
        <parameter name="AIL.Demo.TradePL.TradeQB">General Trades Public Liability product</parameter>
        <parameter name="AIL.Demo.WidgetShowcase">Pageflow widget demonstration product.</parameter>
    </group>
</configuration>
