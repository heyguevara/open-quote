<deployments>
    <deployment>
        <parent-ref />
        <if-exists>keep</if-exists>
        <portal>
            <properties>
                <property>
                    <name>theme.id</name>
                    <value>bluestone</value>
                </property>
                <property>
                    <name>theme.decorationRendererId</name>
                    <value>default</value>
                </property>
                <property>
                    <name>layout.id</name>
                    <value>OneColumn</value>
                </property>
                <property>
                    <name>portal.defaultObjectName</name>
                    <value>Home</value>
                </property>
            </properties>
            <portal-name>Irish Broker PI</portal-name>
            <supported-modes>
                <mode>view</mode>
            </supported-modes>
            <supported-window-states>
                <window-state>normal</window-state>
            </supported-window-states>
            <security-constraint>
                <policy-permission>
                    <action-name>view</action-name>
                    <unchecked />
                </policy-permission>
            </security-constraint>

            <page>
                <page-name>Home</page-name>
                <default>true</default>
                <properties>
                    <property>
                        <name>layout.id</name>
                        <value>OneColumnTwoColumns</value>
                    </property>
                    <property>
                        <name>order</name>
                        <value>1</value>
                    </property>
                </properties>
                <window>
                    <window-name>IrishBrokerPIBannerWindow</window-name>
                	<properties>
                    	<property>
                        	<name>theme.decorationRendererId</name>
                        	<value>emptyRenderer</value>
                    	</property>
					</properties>
                    <instance-ref>IrishBrokerPIBannerInstance</instance-ref>
                    <region>top-center</region>
                    <height>1</height>
                </window>
                <window>
                    <window-name>IrishBrokerPINewsWindow</window-name>
                    <instance-ref>IrishBrokerPINewsInstance</instance-ref>
                    <region>bottom-left</region>
                    <height>1</height>
                </window>
                <window>
                    <window-name>IrishBrokerPIMainMenuWindow</window-name>
                    <instance-ref>IrishBrokerPIMainMenuInstance</instance-ref>
                    <default>true</default>
                    <region>bottom-right</region>
                    <height>1</height>
                </window>
                <window>
                    <window-name>IrishBrokerPILoginWindow</window-name>
                    <instance-ref>IrishBrokerPILoginInstance</instance-ref>
                    <default>true</default>
                    <region>bottom-right</region>
                    <height>2</height>
                </window>
                <security-constraint>
                    <policy-permission>
                        <action-name>viewrecursive</action-name>
                        <unchecked />
                    </policy-permission>
                </security-constraint>
            </page>

            <page>
                <page-name>Products</page-name>
                <properties>
                    <property>
                        <name>order</name>
                        <value>2</value>
                    </property>
                </properties>
                <window>
                    <window-name>IrishBrokerPIAboutWindow</window-name>
                    <instance-ref>IrishBrokerPIAboutInstance</instance-ref>
                    <default>true</default>
                    <region>center</region>
                    <height>1</height>
                </window>
                <security-constraint>
                    <policy-permission>
                        <action-name>viewrecursive</action-name>
                        <unchecked />
                    </policy-permission>
                </security-constraint>
            </page>

            <page>
                <page-name>Services</page-name>
                <properties>
                    <property>
                        <name>order</name>
                        <value>3</value>
                    </property>
                </properties>
                <window>
                    <window-name>IrishBrokerPIAboutWindow</window-name>
                    <instance-ref>IrishBrokerPIAboutInstance</instance-ref>
                    <default>true</default>
                    <region>center</region>
                    <height>1</height>
                </window>
                <security-constraint>
                    <policy-permission>
                        <action-name>viewrecursive</action-name>
                        <unchecked />
                    </policy-permission>
                </security-constraint>
            </page>

            <page>
                <page-name>Contacts</page-name>
                <properties>
                    <property>
                        <name>order</name>
                        <value>3</value>
                    </property>
                </properties>
                <window>
                    <window-name>IrishBrokerPIAboutWindow</window-name>
                    <instance-ref>IrishBrokerPIAboutInstance</instance-ref>
                    <default>true</default>
                    <region>center</region>
                    <height>1</height>
                </window>
                <security-constraint>
                    <policy-permission>
                        <action-name>viewrecursive</action-name>
                        <unchecked />
                    </policy-permission>
                </security-constraint>
            </page>

            <page>
                <page-name>Quotation</page-name>
                <properties>
                    <property>
                        <name>order</name>
                        <value>4</value>
                    </property>
                </properties>
                <window>
                    <window-name>IrishBrokerPIAboutWindow</window-name>
                    <instance-ref>IrishBrokerPIAboutInstance</instance-ref>
                    <default>true</default>
                    <region>center</region>
                    <height>1</height>
                </window>
                <security-constraint>
                    <policy-permission>
                        <role-name>Broker</role-name>
                        <action-name>viewrecursive</action-name>
                    </policy-permission>
                </security-constraint>
            </page>
        </portal>
    </deployment>
</deployments>
