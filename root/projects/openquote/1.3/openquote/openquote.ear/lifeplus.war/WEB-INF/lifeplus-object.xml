<deployments>
    <deployment>
        <parent-ref />
        <if-exists>keep</if-exists>
        <portal>
            <properties>
                <property>
                    <name>theme.id</name>
                    <value>greentree</value>
                </property>
                <property>
                    <name>layout.id</name>
                    <value>generic</value>
                </property>
                <property>
                    <name>portal.defaultObjectName</name>
                    <value>Life Plus</value>
                </property>
            </properties>
            <portal-name>Life Plus</portal-name>
            <supported-modes>
                <mode>view</mode>
                <mode>edit</mode>
                <mode>help</mode>
            </supported-modes>
            <supported-window-states>
                <window-state>normal</window-state>
                <window-state>minimized</window-state>
                <window-state>maximized</window-state>
            </supported-window-states>
            <security-constraint>
                <policy-permission>
                    <action-name>viewrecursive</action-name>
                    <action-name>personalizerecursive</action-name>
                    <unchecked />
                </policy-permission>
            </security-constraint>

            <page>
                <page-name>Life Plus</page-name>
                <default>true</default>
                <properties>
                    <property>
                        <name>layout.id</name>
                        <value>generic</value>
                    </property>
                    <property>
                        <name>order</name>
                        <value>1</value>
                    </property>
                </properties>
                <window>
                    <window-name>LifePlusLeftNavWindow</window-name>
                    <instance-ref>LifePlusLeftNavInstance</instance-ref>
                    <region>left</region>
                    <height>1</height>
                    <properties>
                        <property>
                            <name>theme.windowRendererId</name>
                            <value>default</value>
                        </property>
                        <property>
                            <name>theme.decorationRendererId</name>
                            <value>emptyRenderer</value>
                        </property>
                    </properties>
                </window>
                <window>
                    <window-name>LifePlusTestimonialsWindow</window-name>
                    <instance-ref>LifePlusTestimonialsInstance</instance-ref>
                    <region>left</region>
                    <height>2</height>
                    <properties>
                        <property>
                            <name>theme.windowRendererId</name>
                            <value>default</value>
                        </property>
                        <property>
                            <name>theme.decorationRendererId</name>
                            <value>emptyRenderer</value>
                        </property>
                    </properties>
                </window>
                <window>
                    <window-name>LifePlusBannerWindow</window-name>
                    <instance-ref>LifePlusBannerInstance</instance-ref>
                    <region>center</region>
                    <height>1</height>
                    <properties>
                        <property>
                            <name>theme.windowRendererId</name>
                            <value>default</value>
                        </property>
                        <property>
                            <name>theme.decorationRendererId</name>
                            <value>emptyRenderer</value>
                        </property>
                    </properties>
                </window>
                <window>
                    <window-name>LifePlusQuotationWindow</window-name>
                    <instance-ref>LifePlusQuotationInstance</instance-ref>
                    <default>true</default>
                    <region>center</region>
                    <height>2</height>
                    <properties>
                        <property>
                            <name>theme.windowRendererId</name>
                            <value>default</value>
                        </property>
                        <property>
                            <name>theme.decorationRendererId</name>
                            <value>emptyRenderer</value>
                        </property>
                    </properties>
                </window>
            </page>

            <page>
                <page-name>About Life Plus</page-name>
                <properties>
                    <property>
                        <name>layout.id</name>
                        <value>generic</value>
                    </property>
                    <property>
                        <name>order</name>
                        <value>2</value>
                    </property>
                </properties>
                <window>
                    <window-name>LifePlusLeftNavWindow2</window-name>
                    <instance-ref>LifePlusLeftNavInstance</instance-ref>
                    <region>left</region>
                    <height>1</height>
                    <properties>
                        <property>
                            <name>theme.windowRendererId</name>
                            <value>default</value>
                        </property>
                        <property>
                            <name>theme.decorationRendererId</name>
                            <value>emptyRenderer</value>
                        </property>
                    </properties>
                </window>
                <window>
                    <window-name>LifePlusAboutLifePlusWindow</window-name>
                    <instance-ref>LifePlusAboutLifePlusInstance</instance-ref>
                    <default>true</default>
                    <region>center</region>
                    <height>2</height>
                    <properties>
                        <property>
                            <name>theme.windowRendererId</name>
                            <value>default</value>
                        </property>
                        <property>
                            <name>theme.decorationRendererId</name>
                            <value>emptyRenderer</value>
                        </property>
                    </properties>
                </window>
            </page>

            <page>
                <page-name>About Us</page-name>
                <properties>
                    <property>
                        <name>layout.id</name>
                        <value>generic</value>
                    </property>
                    <property>
                        <name>order</name>
                        <value>3</value>
                    </property>
                </properties>
                <window>
                    <window-name>LifePlusLeftNavWindow</window-name>
                    <instance-ref>LifePlusLeftNavInstance</instance-ref>
                    <region>left</region>
                    <height>1</height>
                    <properties>
                        <property>
                            <name>theme.windowRendererId</name>
                            <value>default</value>
                        </property>
                        <property>
                            <name>theme.decorationRendererId</name>
                            <value>emptyRenderer</value>
                        </property>
                    </properties>
                </window>
                <window>
                    <window-name>LifePlusAboutUsWindow</window-name>
                    <instance-ref>LifePlusAboutUsInstance</instance-ref>
                    <default>true</default>
                    <region>center</region>
                    <height>2</height>
                    <properties>
                        <property>
                            <name>theme.windowRendererId</name>
                            <value>default</value>
                        </property>
                        <property>
                            <name>theme.decorationRendererId</name>
                            <value>emptyRenderer</value>
                        </property>
                    </properties>
                </window>
            </page>
        </portal>
    </deployment>
</deployments>
