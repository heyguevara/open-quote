<?xml version="1.0" encoding="UTF-8"?>
<!--
 This file defines the default configuration used by the Quotation EJB component.
-->
<configuration xsi:noNamespaceSchemaLocation="http://www.appliedindustriallogic.com/schemas/Configuration.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>Quotation EJB configuration</name>
    <timeout>-1</timeout>
    <who>factory</who>
    <version>$Revision: 1.4 $</version>
    <types>
        <command name="AssessRisk" builder="ClassBuilder" key="com.ail.insurance.quotation.AssessRiskCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.AssessRiskService</parameter>
        </command>
        <command name="CalculatePremium" builder="ClassBuilder" key="com.ail.insurance.quotation.CollectPremiumCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.CollectPremiumService</parameter>
        </command>
        <command name="CalculateTax" builder="ClassBuilder" key="com.ail.insurance.quotation.CalculateTaxCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.CalculateTaxService</parameter>
        </command>
        <command name="CalculateCommission" builder="ClassBuilder" key="com.ail.insurance.quotation.CalculateCommissionCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.CalculateCommissionService</parameter>
        </command>
        <command name="CalculateBrokerage" builder="ClassBuilder" key="com.ail.insurance.quotation.CalculateBrokerageCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.CalculateBrokerageService</parameter>
        </command>
        <command name="CalculateManagementCharge" builder="ClassBuilder" key="com.ail.insurance.quotation.CalculateManagementChargeCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.CalculateManagementChargeService</parameter>
        </command>
        <command name="AddQuoteNumber" builder="ClassBuilder" key="com.ail.insurance.quotation.AddQuoteNumberCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.AddQuoteNumberService</parameter>
        </command>
        <command name="AddPolicyNumber" builder="ClassBuilder" key="com.ail.insurance.quotation.AddPolicyNumberCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.AddPolicyNumberService</parameter>
        </command>
        <command name="EnforceCompliance" builder="ClassBuilder" key="com.ail.insurance.quotation.EnforceComplianceCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.EnforceComplianceService</parameter>
        </command>
        <command name="GenerateDocument" builder="ClassBuilder" key="com.ail.insurance.quotation.GenerateDocumentCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.GenerateQuoteService</parameter>
        </command>
    </types>
</configuration>
