<?xml version="1.0" encoding="UTF-8"?>
<!--
 This file defines the default configuration used by the Quotation EJB component.
-->
<configuration xsi:noNamespaceSchemaLocation="http://www.appliedindustriallogic.com/schemas/Configuration.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>Quotation EJB configuration</name>
    <timeout>-1</timeout>
    <who>factory</who>
    <version>$Revision: 1.4 $</version>
    <types>
        <command name="AssessRisk" builder="ClassBuilder" key="com.ail.insurance.quotation.assessrisk.AssessRiskCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.assessrisk.AssessRiskService</parameter>
        </command>
        <command name="CalculatePremium" builder="ClassBuilder" key="com.ail.insurance.acceptance.CollectPremiumCommandImpl">
            <parameter name="Service">com.ail.insurance.acceptance.CollectPremiumService</parameter>
        </command>
        <command name="CalculateTax" builder="ClassBuilder" key="com.ail.insurance.quotation.calculatetax.CalculateTaxCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.calculatetax.CalculateTaxService</parameter>
        </command>
        <command name="CalculateCommission" builder="ClassBuilder" key="com.ail.insurance.quotation.calculatecommission.CalculateCommissionCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.calculatecommission.CalculateCommissionService</parameter>
        </command>
        <command name="CalculateBrokerage" builder="ClassBuilder" key="com.ail.insurance.quotation.calculatebrokerage.CalculateBrokerageCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.calculatebrokerage.CalculateBrokerageService</parameter>
        </command>
        <command name="CalculateManagement" builder="ClassBuilder" key="com.ail.insurance.quotation.calculatemanagementcharge.CalculateManagementChargeCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.calculatemanagementcharge.CalculateManagementChargeService</parameter>
        </command>
        <command name="AddQuoteNumber" builder="ClassBuilder" key="com.ail.insurance.quotation.addquotenumber.AddQuoteNumberCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.addquotenumber.AddQuoteNumberService</parameter>
        </command>
        <command name="AddPolicyNumber" builder="ClassBuilder" key="com.ail.insurance.quotation.addpolicynumber.AddPolicyNumberCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.addpolicynumber.AddPolicyNumberService</parameter>
        </command>
        <command name="EnforceCompliance" builder="ClassBuilder" key="com.ail.insurance.quotation.enforcecompliance.EnforceComplianceCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.enforcecompliance.EnforceComplianceService</parameter>
        </command>
        <command name="GenerateDocument" builder="ClassBuilder" key="com.ail.insurance.quotation.generatedocument.GenerateDocumentCommandImpl">
            <parameter name="Service">com.ail.insurance.quotation.generatedocument.GenerateDocumentService</parameter>
        </command>
    </types>
</configuration>
