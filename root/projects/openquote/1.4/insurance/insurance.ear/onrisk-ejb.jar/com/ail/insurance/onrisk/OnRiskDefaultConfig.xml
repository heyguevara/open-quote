<?xml version="1.0" encoding="UTF-8"?>
<!--
 This file defines the default configuration used by the OnRisk EJB component.
-->
<configuration xsi:noNamespaceSchemaLocation="http://www.appliedindustriallogic.com/schemas/Configuration.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>OnRisk EJB configuration</name>
    <timeout>-1</timeout>
    <who>factory</who>
    <version>$Revision: 1.4 $</version>
    <types>
        <command name="GenerateInvoice" builder="ClassBuilder" key="com.ail.insurance.onrisk.invoice.GenerateInvoiceCommandImpl">
            <parameter name="Service">com.ail.insurance.onrisk.invoice.GenerateInvoiceService</parameter>
        </command>
        <command name="GenerateWording" builder="ClassBuilder" key="com.ail.insurance.onrisk.wording.GenerateWordingCommandImpl">
            <parameter name="Service">com.ail.insurance.onrisk.wording.GenerateWordingSrevice</parameter>
        </command>
        <command name="GenerateCertificate" builder="ClassBuilder" key="com.ail.insurance.onrisk.certificate.GenerateCertificateCommandImpl">
            <parameter name="Service">com.ail.insurance.onrisk.certificate.GenerateCertificateService</parameter>
        </command>
    </types>
</configuration>
