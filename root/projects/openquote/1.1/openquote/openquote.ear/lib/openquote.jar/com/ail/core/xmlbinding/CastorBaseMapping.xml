    <class name="com.ail.openquote.PersonalProposer" extends="com.ail.party.Person" auto-complete="false">
        <field name="emailAddress" type="string"/>
        <field name="telephoneNumber" type="string"/>
        <field name="mobilephoneNumber" type="string"/>
    </class>

    <class name="com.ail.openquote.CommercialProposer" extends="com.ail.party.Organisation" auto-complete="false">
        <field name="emailAddress" type="string"/>
        <field name="telephoneNumber" type="string"/>
        <field name="mobilephoneNumber" type="string"/>
        <field name="firstName" type="string"/>
        <field name="surname" type="string"/>
        <field name="title" type="string" get-method="getTitleAsString" set-method="setTitleAsString"/>
        <field name="otherTitle" type="string"/>
    </class>

    <class name="com.ail.openquote.Broker" extends="com.ail.party.Organisation" auto-complete="false">
        <map-to xml="broker"/>
        <field name="tradingName" type="string"/>
        <field name="telephoneNumber" type="string"/>
        <field name="paymentTelephoneNumber" type="string"/>
        <field name="quoteTelephoneNumber" type="string"/>
        <field name="claimTelephoneNumber" type="string"/>
        <field name="quoteEmailAddress" type="string"/>
        <field name="contactName" type="string"/>
        <field name="directDebitIdentificationNumber" type="string"/>
