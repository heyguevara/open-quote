<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
	<managed-bean>
		<managed-bean-name>form</managed-bean-name>
		<managed-bean-class>com.ail.core.ui.ConfigureForm</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mgr</managed-bean-name>
		<managed-bean-class>com.ail.core.ui.ConfigureManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<navigation-rule>
		<from-view-id>/pages/list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>resetAction.success</from-outcome>
			<to-view-id>/pages/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>resetAllAction.failure</from-outcome>
			<to-view-id>/pages/error.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>clearCacheAction.success</from-outcome>
			<to-view-id>/pages/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>clearCacheAction.failure</from-outcome>
			<to-view-id>/pages/error.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>resetConfigurationAction.success</from-outcome>
			<to-view-id>/pages/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>resetConfigurationAction.failure</from-outcome>
			<to-view-id>/pages/error.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>resetAction.success</from-outcome>
			<to-view-id>/pages/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editLatestAction.success</from-outcome>
			<to-view-id>/pages/edit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editLatestAction.failure</from-outcome>
			<to-view-id>/pages/error.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>historyFromListAction.success</from-outcome>
			<to-view-id>/pages/history.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>historyFromListAction.failure</from-outcome>
			<to-view-id>/pages/error.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/edit.jsp</from-view-id>
		<navigation-case>
			<from-outcome>backToListAction.success</from-outcome>
			<to-view-id>/pages/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>historyFromEditorAction.success</from-outcome>
			<to-view-id>/pages/history.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>historyFromEditorAction.failure</from-outcome>
			<to-view-id>/pages/error.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editorSaveAction.success</from-outcome>
			<to-view-id>/pages/edit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editorSaveAction.failure</from-outcome>
			<to-view-id>/pages/error.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/history.jsp</from-view-id>
		<navigation-case>
			<from-outcome>backToListAction.success</from-outcome>
			<to-view-id>/pages/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewHistoricalAction.success</from-outcome>
			<to-view-id>/pages/edit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewHistoricalAction.failure</from-outcome>
			<to-view-id>/pages/error.jsp</to-view-id>
		</navigation-case>
  	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/download.jsp</from-view-id>
		<navigation-case>
			<from-outcome>downloadSelectedAction.failure</from-outcome>
			<to-view-id>/pages/error.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>